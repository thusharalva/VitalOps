    "date-fns": "^3.0.0",
    "morgan": "^1.10.0",
    "helmet": "^7.1.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.5",
    "@types/morgan": "^1.9.9",
